        g = J' * residuals
        ŒîŒ∏_test = -H \ g
        
        # Check residual norm
        r_norm = norm(J * ŒîŒ∏_test + residuals)
        
        if r_norm < target_residual
            Œª_max = Œª_mid
        else
            Œª_min = Œª_mid
        end
        Œª_opt = Œª_mid
    end
    
    # Final solution with optimal Œª
    H = J' * J + Œª_opt * I
    g = J' * residuals
    ŒîŒ∏ = -H \ g
    
