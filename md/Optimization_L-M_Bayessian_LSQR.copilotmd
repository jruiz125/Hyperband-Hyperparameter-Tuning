# Define the ODE
function ude_dynamics!(du, u, p, s)
    NN_FORWARD_COUNT[] += 1
    # Add numerical stability checks
    if any(!isfinite, u)
        du .= 0.0f0
        return nothing
    end
    # Network prediction
    √ª_1 = U(u, p, _st)[1] 
    
    # Evaluate differential equations
    du[1] = √ª_1[1]   # dx/ds
    du[2] = √ª_1[2]   # dy/ds
    du[3] = u[4]     # dŒ∏/ds = Œ∫
    du[4] = √ª_1[3]   # dŒ∫/ds
    return nothing
end 

